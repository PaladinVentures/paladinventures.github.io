  <languages folder="language">
    <language tag="ar-AA">ar-AA/ar-AA.tpl_shakenup.ini</language>
    <language tag="de-DE">de-DE/de-DE.tpl_shakenup.ini</language>
    <language tag="en-GB">en-GB/en-GB.tpl_shakenup.ini</language>
    <language tag="es-ES">es-ES/es-ES.tpl_shakenup.ini</language>
    <language tag="fr-FR">fr-FR/fr-FR.tpl_shakenup.ini</language>
    <language tag="gr-GR">gr-GR/gr-GR.tpl_shakenup.ini</language>
    <language tag="it-IT">it-IT/it-IT.tpl_shakenup.ini</language>
    <language tag="ja-JP">ja-JP/ja-JP.tpl_shakenup.ini</language>
    <language tag="nb-NO">nb-NO/nb-NO.tpl_shakenup.ini</language>
    <language tag="nl-NL">nl-NL/nl-NL.tpl_shakenup.ini</language>
    <language tag="pt-PT">pt-PT/pt-PT.tpl_shakenup.ini</language>
    <language tag="ru-RU">ru-RU/ru-RU.tpl_shakenup.ini</language>
    <language tag="sv-SE">sv-SE/sv-SE.tpl_shakenup.ini</language>
    <language tag="zh-CN">zh-CN/zh-CN.tpl_shakenup.ini</language>
  </languages>
  <files>
    <folder>css</folder>
	<folder>fonts</folder>
    <folder>html</folder>
    <folder>images</folder>
    <folder>js</folder>
    <folder>language</folder>
    <folder>php</folder>
    <filename>favicon.ico</filename>
    <filename>index.php</filename>
    <filename>index.html</filename>
    <filename>templateDetails.xml</filename>
    <filename>template_preview.png</filename>
    <filename>template_thumbnail.png</filename>
  </files>
  <positions>
    <position>js-headermenu</position>
    <position>js-navigationmenu</position>
    <position>js-slideshow</position>
	<position>js-blog</position>
	<position>js-blogmasonry</position>
    <position>js-contactform</position>
    <position>js-contactinfo</position>
    <position>js-map</position>
	<position>js-right</position>
    <position>js-left</position>
    <position>js-footer</position>
    <!-- Custom modules -->
    <position>js-aboutwrapper</position>
	<position>js-portfoliowrapper</position>
    <position>js-teamwrapper</position>
    <position>js-numberswrapper</position>
    <position>js-serviceswrapper</position>
    <position>js-showcasewrapper</position>
    <position>js-testimonialswrapper</position>
    <position>js-learnmorewrapper</position>
    <position>js-informationwrapper</position>
    <position>js-buythemewrapper</position>
    <position>js-clientswrapper</position>
    <!-- End Custom modules -->
  </positions>
  <config>
    <fields name="params">
      <fieldset name="LOGOOPTIONS">
        <field label="LOGOINFO"  type="spacer" class="alert alert-blue"/>
        <field name="navigationlogo" class="" type="media" default=""
					label="NAVIGATIONLOGO"
					description="Add Navigation Logo" />
        <field name="navigationmoblogo" class="" type="media" default=""
					label="NAVIGATIONMOBLOGO"
					description="Add Navigation Mobile Logo" />
      </fieldset>
      <fieldset name="COLOROPTIONS">
        <field label="COLORINFO"  type="spacer" class="alert alert-blue"/>
        <field name="themecolor" type="list" default="blu" label="SELECT THEME COLOR CODE" description="Select Theme Color" >
          <option value="default">Default</option>
          <option value="blue">blue</option>
          <option value="bridge">bridge</option>
          <option value="cyan">cyan</option>
          <option value="darkred">darkred</option>
          <option value="green">green</option>
          <option value="lightblue">lightblue</option>
          <option value="orange">orange</option>
          <option value="pink">pink</option>
          <option value="purple">purple</option>
          <option value="slate">slate</option>
          <option value="yellow">yellow</option>
        </field>
      </fieldset>
      <fieldset name="SOCIALOPTIONS">
        <field label="SOCIALINFO"  type="spacer" class="alert-blue"/>
        <field name="social" type="radio" label="SHOWSOCIAL" description="Enable / Disable social icon" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="nav_facebook_sw" type="radio" label="Show Facebook" description="Facebook" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="facebook" type="text" label="Facebook Url" description="Facebook link" size="50" />
        <field name="nav_twitter_sw" type="radio" label="Show Twitter" description="Twitter" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="twitter" type="text" label="Twitter Url" description="Twitter link" size="50" />
        <field name="nav_google_sw" type="radio" label="Show Google Plus" description="Google" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="google" type="text" label="Google Plus Url" description="Google plus link" size="50" />
        <field name="nav_linkedin_sw" type="radio" label="Show LinkedIn" description="Link of Delicious" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="linkedin" type="text" label="LinkedIn Url" description="Linkedin link" size="50" />
        <field name="nav_skype_sw" type="radio" label="Show Skype" description="SKYPE" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="skype" type="text" label="Skype Url" description="Skype Id" size="50" />
        <field name="nav_flicker_sw" type="radio" label="Show Flickr" description="Flicker" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="flicker" type="text" label="Show Flickr" description="Flicker Id" size="50" />
        <field name="nav_html5_sw" type="radio" label="Show Html5" description="HTML5" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="html5" type="text" label="Html5" description="Link of HTML5" size="50" />
        <field name="nav_youtube_sw" type="radio" label="Show Youtube" description="Link of youtube" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="youtube" type="text" label="Youtube Url" description="Link of Youtube Channel" size="50" />
        <field name="nav_rss_sw" type="radio" label="Show Rss Feed" description="RSS Feed" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="rss" type="text" label="Rss Feed" description="Link of Rss-Feed" size="50" />
        <field name="nav_rss_behance" type="radio" label="Show Behance" description="Behance" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="behance" type="text" label="Behance" description="Link of Behance" size="50" />
        <field name="nav_rss_dribbble" type="radio" label="Show Dribble" description="Dribble" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="dribbble" type="text" label="Dribble" description="Link of Dribble" size="50" />
      </fieldset>
      <fieldset name="COPYRIGHT">
        <field label="COPYRIGHT_INFO"  type="spacer" class="alert alert-blue"/>
        <field name="copyright" type="text" size="50" filter="raw" label="COPYRIGHT TEXT" description="Copyright Text" />
      </fieldset>
      <fieldset name="CONTACTOPTIONS">
        <field label="CONTACTINFO"  type="spacer" class="alert-blue"/>
        <field name="ctitle" type="text" default="Contact us" label="CTITLE" description="Contact Title"  size="50"/>
        <field name="cdesc" type="text" label="CDESC" default="IF YOU HAVE ANY QUESTIONS, JUST FEEL FREE TO LEAVE US A MESSAGE" description="Contact Description"  size="50"/>
        <field name="cagencytitle" type="text" default="OUR AGENCY" label="CAGENCYTITLE" description="Agency Title"  size="50"/>
        <field name="cagencydesc" type="text" default="You don't get sick, I do. That's also clear. But for some reason, you and I react the exact same way to water. We swallow it too fast, we choke. We get some in our lungs, we drown. However unreal it may seem, we are connected, you and I. We're on the same curve, just on opposite ends." label="CAGENCYDESC"	description="Agency Description" size="100" />
        <field name="calltitle" type="text" default="CALL US" label="CALLTITLE"	description="Call Title" size="50" />
        <field name="call1" type="text" default="+0 123.555.950" label="CALL1"	description="Call 1" size="50" />
        <field name="call2" type="text" default="+0 987.400.300" label="CALL2"	description="Call 2" size="50" />
        <field name="visittitle" type="text" default="VISIT US" label="VISITTITLE"	description="Visit Title" size="50" />
        <field name="visitadd" type="text" default="123 Street Road, Manhattan" label="VISITADD"	description="Visit Address" size="50" />
        <field name="visitcity" type="text" default="New York City" label="VISITCITY"	description="Visit City" size="50" />
        <field name="visitcode" type="text" default="12-500" label="VISITCODE"	description="Visit Code" size="50" />
        <field name="visitcountry" type="text" default="United States" label="VISITCOUNTRY"	description="Visit Country" size="50" />
      </fieldset>
      <fieldset name="TEXT_WIDGET">
        <field label="TEXT_WIDGET_INFO"  type="spacer" class="alert-blue"/>
        <field name="text_widget_description" type="textarea" rows="5" cols="30" default="TEXT_WIDGET_DESCRIPTION" label="Text Widget description" description="Enter the Text Widget" filter="raw" />
      </fieldset>
    </fields>
  </config>
</extension>
